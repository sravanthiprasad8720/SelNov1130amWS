package nov7th;

public class MethodsDemo1 {

	public static void main(String[] args) {
		
		int num1=5;
		
		int res1=0;
		
		//for(intial value;condition;for every iteration increment or decrement )
		for(int i=1;i<=5;i++)
		{
			res1=res1+i;
		}
		
		System.out.println("Sum of "+num1+" Natural numbers is "+res1);

	}

}
